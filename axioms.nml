Variable n:ℕ
Variable m:ℕ\{0}

Axiom (SuccessorsOfPredecessors)
  successor(predecessor(m)) = m

Axiom (PredecessorsOfSuccessors)
  predecessor(successor(n)) = n

Variable k:ℕ

Axiom (ZeroOrSuccessor)
  n = zero ∨ n = successor(k)

Axiom (NonZeroNaturalNumberInfersNotZero)
  Suppose
    n:ℕ\{0}
  Then
    n ≠ zero
